    "prettier": "2.1.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~3.8.3"
  },
  "description": "Miftah Standalone Donation Application",
  "cordova": {
    "plugins": {
